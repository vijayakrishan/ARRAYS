#include <stdio.h>
#define MAX 50

int main(){
int a[MAX][MAX], b[MAX][MAX];
int rows, columns, i, j;

printf("Enter the number of rows and columns:");
scanf("%d%d", &rows, &columns);

for(i=0;i<rows;i++){
for(j=0;j<columns;j++){
scanf("%d",&a[i][j]);
}}

for(i=0;i<rows;i++){
for(j=0;j<columns;j++){
scanf("%d",&b[i][j]);
}}

printf("Result:\n");
for(i=0;i<rows;i++){
for(j=0;j<columns;j++){
sum[i][j]= a[i][j] + b[i][j];
printf("%d ",sum[i][j]);
}
printf("\n");
}
return 0;
}

